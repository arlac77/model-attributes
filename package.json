{
  "name": "model-attributes",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/attributes.mjs",
  "module": "src/attributes.mjs",
  "description": "attribute meta description",
  "keywords": [
    "meta",
    "attribute",
    "model"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/attributes.mjs --section=API",
    "lint": "documentation lint src/attributes.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.4.1",
    "c8": "^3.5.0",
    "documentation": "^10.1.0",
    "esm": "^3.2.22",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^15.13.12"
  },
  "engines": {
    "node": ">=10.15.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/model-attributes.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/model-attributes/issues"
  },
  "homepage": "https://github.com/arlac77/model-attributes#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  },
  "_release": {
    "verifyRelease": "cracks"
  }
}
