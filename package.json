{
  "name": "model-attributes",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/attributes.mjs",
  "exports": {
    ".": "./src/attributes.mjs"
  },
  "description": "attribute meta description",
  "keywords": [
    "attribute",
    "meta",
    "model"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/attributes.mjs --section=API",
    "lint": "documentation lint ./src/attributes.mjs"
  },
  "devDependencies": {
    "ava": "^3.8.2",
    "c8": "^7.2.0",
    "documentation": "^13.0.1",
    "semantic-release": "^17.0.8"
  },
  "engines": {
    "node": ">=12.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/model-attributes.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/model-attributes/issues"
  },
  "homepage": "https://github.com/arlac77/model-attributes#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
