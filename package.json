{
  "name": "model-attributes",
  "version": "0.0.0-semantic-release",
  "description": "attribute meta description",
  "keywords": [
    "meta",
    "attribute",
    "model"
  ],
  "main": "dist/attributes.js",
  "jsnext:main": "src/attributes.js",
  "module": "src/attributes.js",
  "scripts": {
    "cover": "npm run build && ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "doc": "jsdoc src/*.js",
    "build": "./node_modules/rollup/bin/rollup --format=cjs --output=dist/attributes.js -- src/attributes.js",
    "test": "npm run build && mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t README.hbs -f src/attributes.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/model-attributes.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/model-attributes/issues"
  },
  "homepage": "https://github.com/arlac77/model-attributes#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "cracks": "3.1.2",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "markdown-doctest": "^0.9.0",
    "mocha": "^3.1.2",
    "rollup": "^0.36.2",
    "semantic-release": "^6.3.1",
    "jsdoc-to-markdown": "^2.0.1"
  },
  "release": {
    "verifyRelease": "cracks"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}