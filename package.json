{
  "name": "model-attributes",
  "version": "0.0.0-semantic-release",
  "description": "attribute meta description",
  "keywords": [
    "meta",
    "attribute",
    "model"
  ],
  "main": "dist/attributes.js",
  "module": "src/attributes.js",
  "scripts": {
    "cover": "npm run build && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "build": "node_modules/rollup/bin/rollup --output=dist/attributes.js -c -- src/attributes.js",
    "test": "npm run build && node_modules/.bin/mocha tests/*_test.js && markdown-doctest",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/model-attributes.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/model-attributes/issues"
  },
  "homepage": "https://github.com/arlac77/model-attributes#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "cracks": "3.1.2",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.2.0",
    "rollup": "^0.37.2",
    "semantic-release": "^6.3.5"
  },
  "release": {
    "verifyRelease": "cracks"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}