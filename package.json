{
  "name": "model-attributes",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/attributes.js",
  "module": "src/attributes.js",
  "description": "attribute meta description",
  "keywords": [
    "meta",
    "attribute",
    "model"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/attributes.js --section=API",
    "lint": "documentation lint src/attributes.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "pretest": "rollup -c tests/rollup.config.js",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.1.0",
    "c8": "^3.4.0",
    "cracks": "3.1.2",
    "documentation": "^9.1.1",
    "esm": "^3.1.3",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.1.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.4.4",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "semantic-release": "^15.13.3"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/model-attributes.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/model-attributes/issues"
  },
  "homepage": "https://github.com/arlac77/model-attributes#readme",
  "release": {
    "verifyRelease": "cracks"
  },
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
